{
  "name": "client",
  "version": "0.0.1",
  "description": "vedo client",
  "author": "william",
  "license": "ISC",
  "typings": "./src/typings.d.ts",
  "scripts": {
    "dev:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "dev:preload": "cross-env NODE_ENV=development vite build --config ./.erb/config/vite.config.preload.dev.ts --watch",
    "line": "===================================================================",
    "dev:start": "cross-env NODE_ENV=development vite --config ./.erb/config/vite.config.renderer.prod.ts",
    "build:main": "vite build --config ./.erb/config/vite.config.main.prod.ts",
    "build:renderer": "vite build --config ./.erb/config/vite.config.renderer.prod.ts",
    "build:release": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "prepackage": "cross-env NODE_ENV=production npm run build:release",
    "package": "electron-builder build",
    "line1": "========================web 打包====================================",
    "dev:web": "cross-env NODE_ENV=development vite --config ./.erb/config/vite.config.web.ts",
    "build:web": "vite build --config ./.erb/config/vite.config.web.ts",
    "electron:generate-icons": "electron-icon-builder --input=./assets/icon.png --output=build --flatten"
  },
  "dependencies": {
    "electron-debug": "^3.2.0",
    "electron-log": "5.0.0-beta.16",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.32",
    "source-map-support": "^0.5.21",
    "ui": "workspace:*",
    "utils": "workspace:*",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "electron": "24.0.0",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint-config-custom": "workspace:*",
    "less": "^4.1.3",
    "tsconfig": "workspace:*",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite-plugin-style-import": "^2.0.0"
  }
}
